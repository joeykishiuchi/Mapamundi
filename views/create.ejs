<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Create A Map</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <link rel="stylesheet" href="../styles/create.css" type="text/css" >
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>

  <body>
    <header>
      <%- include('partials/nav') %>
    </header>
    <main class="createbox">
      <div class="column">
      <h1>Start a Map</h1>
      <form class="form" action="/create" method="POST">
        <div class="form-group mb-2">
          <input class="form-control" type="title" name="title" placeholder="Name">
        </div>
        <div class="form-group mb-2">
          <input class="form-control" type="description" name="description" placeholder="Description">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
    </main>
  </body>
</html>
